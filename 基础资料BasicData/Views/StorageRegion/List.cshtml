@{
    Layout = "~/Views/Shared/_Index.cshtml";
}

<div id="apps" class="pt-5 pl-5 pr-5">
    <div class="row m-0">
        <div class="col-md-12 col-sm-12 row-tit p-0 mb-5">
            <div id="T_Menu_Alias" class="col-md-2 col-sm-2 p-tit">仓库储区列表 </div>
            <div class="col-md-10 col-sm-10 p-btn pr-40">
                <erp-button v-bind:btn_cb="New" v_id="btnNew" btn_opt="新增" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b" fa_icon="fa fa-plus">新增</erp-button>
                <erp-button v-bind:btn_cb="BatchNew" v_id="btnBatchNew" btn_opt="批量新增" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b" fa_icon="fa fa-plus">批量新增</erp-button>
                <erp-button v-bind:btn_cb="Update" v_id="btnUpdate" btn_opt="修改" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b" fa_icon="fa fa-edit">修改</erp-button>
                <erp-button v-bind:btn_cb="Delete" v_id="btnDelete" btn_opt="删除" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b" fa_icon="fa fa-trash">删除</erp-button>
                <erp-button v-bind:btn_cb="Import" v_id="btnImport" btn_opt="导入" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b" fa_icon="fa fa-mail-reply">导入</erp-button>
                <erp-button v-bind:btn_cb="Export" v_id="btnExport" btn_opt="导出" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b" fa_icon="fa fa-mail-forward">导出</erp-button>
                <erp-button v-bind:btn_cb="Set" v_id="btnSet" btn_opt="设置" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b" fa_icon="fa fa-cog">设置</erp-button>
            </div>
            <div class="down"><i class="fa fa-angle-double-down"></i></div>
        </div>
        <div class="col-md-12 col-sm-12 pl-5">
            <div class="tile-body2 clearfix">
                <form class="clearfix" id="searchForm">
                    <div id="order_1" class="form-group show-order pull-left">
                        <erp-text v_id="Storage_Region_Name" v-model="Storage_Region_Name" field_name="B_Storage_Region.Storage_Region_Name" query_type="01" expression="01">仓库储区</erp-text>
                    </div>
                    <div id="order_2" class="form-group show-order pull-left">
                        <erp-text v_id="Storage_Region_Short_Name" v-model="Storage_Region_Short_Name" field_name="B_Storage_Region.Storage_Region_Short_Name" query_type="01" expression="02">仓库储区简称</erp-text>
                    </div> 
                    <div id="order_3" class="form-group show-order pull-left">
                        <erp-multselect v_id="Storage_Region_Type" v-model="Storage_Region_Type" field_name="B_Storage_Region.Storage_Region_Type" query_type="01" expression="04" bind_type="DataDictionary" table_name="B_Storage_Region" system_key="BasicData" v-bind:main_where="StorageRegionWhere" v-bind:main_orderby="StorageRegionOrder" v-bind:Filed_Result="StorageRegionResult" dictionary_Key="STORAGE_REGION_TYPE">储区类型</erp-multselect>
                    </div>
                    <div id="order_4" class="form-group show-order pull-left" >
                        <erp-search class="search" v-bind:template_selection="TemplateSelection" v-model="Storage_Id" v_id="Storage_Id" v-bind:onchange="ChangeStorage" v-bind:main_orderby="mainOrderBy" bind_type="CustomTable" table_name="B_Storage" system_key="BasicData" v-bind:filed_seach="filedSearch" v-bind:filed_result="Storageresult" v-bind:input_field="inputField" isselect="true">仓库</erp-search>
                    </div>
                    <div id="order_5" class="form-group show-order pull-left" >
                        <erp-multselect v_id="Storage_Area_Id" v-model="Storage_Area_Id" field_name="B_Storage_Region.Storage_Area_Id" query_type="01" expression="04" v-bind:linkage="linkage" bind_type="CustomTable" table_name="B_Storage_Area" system_key="BasicData" v-bind:main_where="StorageAreaWhere" v-bind:main_orderby="StorageAreaOrder" v-bind:Filed_Result="StorageAreaResult">仓库区域</erp-multselect>
                    </div>
                </form>
                <div class="submitSearch">
                    <erp-button v-bind:btn_cb="Search" v_id="btnSearch" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b" fa_icon="fa fa-search">查询</erp-button>
                </div>
            </div>
        </div>
        <div class="col-md-12 col-sm-12 p-0">
            <erp-table showchild="false" multiselect="true" showcheckbox="true" v-bind:searchmodel="searchModel" v_id="StorageRegionList" v-bind:tableurl="tableurl" v-bind:columheaderseach="ColumHeaderSeach"></erp-table>
        </div>
    </div>
</div>
@Scripts.Render("~/Areas/BasicData/Content/js/StorageRegionList.js?v=" + ViewData["jsVersion"])
@{
    Layout = "~/Views/Shared/_Index.cshtml";
}
<div id="apps" class="pt-5 pl-5 pr-5">
    <div class="row m-0">
        <div class="col-md-12 col-sm-12 row-tit p-0 mb-5">
            <div id="T_Menu_Alias" class="col-md-2 col-sm-2 p-tit">物流运费列表 </div>
            <div class="col-md-10 col-sm-10 p-btn pr-40">
                <erp-button v-bind:btn_cb="New" v_id="btnNew" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b" fa_icon="fa fa-plus">新增</erp-button>
                <erp-button v-bind:btn_cb="Update" v_id="btnUpdate" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b" fa_icon="fa fa-edit">修改</erp-button>
                <erp-button v-bind:btn_cb="Delete" v_id="btnDelete" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b" fa_icon="fa fa-trash">删除</erp-button>
                <erp-button v-bind:btn_cb="Import" v_id="btnImport" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b" fa_icon="fa fa-mail-reply">导入</erp-button>
                <erp-button v-bind:btn_cb="Export" v_id="btnExport" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b" fa_icon="fa fa-mail-forward">导出</erp-button>
                <erp-button v-bind:btn_cb="Set" v_id="btnSet" btn_opt="设置" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b" fa_icon="fa fa-cog">设置</erp-button>
            </div>
            <div class="down"><i class="fa fa-angle-double-down"></i></div>
        </div>
        <div class="col-md-12 col-sm-12 pl-5">
            <div class="tile-body2 clearfix">
                <form class="clearfix" id="searchForm">
                    <div id="order_1" class="form-group show-order pull-left">
                        <erp-select ismustselect="true" field_name="B_Logistics_Carriage.Carrier_Id" query_type="01" expression="02" v-model="Carrier_Id" bind_type="CustomTable" table_name="B_Carrier" system_key="BasicData" v-bind:main_where="carrierWhere" v-bind:main_orderby="carrierOrder" v-bind:Filed_Result="carrierResult" v_id="Carrier_Id">承运商名称</erp-select>
                    </div>
                    <div id="order_2" class="form-group show-order pull-left">
                        <erp-select ismustselect="true" field_name="B_Logistics_Carriage.Product_Id" query_type="01" expression="02" v-model="Product_Id" bind_type="CustomTable" table_name="B_Product" system_key="BasicData" v-bind:main_where="productWhere" v-bind:main_orderby="productOrder" v-bind:Filed_Result="productResult" v_id="Product_Id">商品名称</erp-select>
                    </div>
                    <div id="order_3" class="form-group show-order pull-left">
                        <erp-select ismustselect="true" field_name="B_Logistics_Carriage.Area" query_type="01" expression="02" v-model="Area" bind_type="CustomTable" table_name="B_Geography" system_key="BasicData" v-bind:main_where="areaWhere" v-bind:main_orderby="areaOrder" v-bind:Filed_Result="areaResult" v_id="Area">区域名称</erp-select>
                    </div>
                    <div id="order_4" class="form-group show-order pull-left">
                        <erp-select ismustselect="true" field_name="B_Logistics_Carriage.Province" query_type="01" expression="02" v-model="Province" bind_type="CustomTable" table_name="B_Geography" system_key="BasicData" v-bind:main_where="provenceWhere" v-bind:main_orderby="provenceOrder" v-bind:Filed_Result="provenceResult" v_id="Province">省名称</erp-select>
                    </div>
                </form>
                <div class="submitSearch">
                    <erp-button v-bind:btn_cb="Search" v_id="btnSearch" fa_icon="fa fa-search" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b">查询</erp-button>
                </div>
            </div>
        </div>
        <div class="col-md-12 col-sm-12 p-0">
            <erp-table multiselect="true" showcheckbox="true" v-bind:searchmodel="searchModel" v_id="LogisticsCarriageList" v-bind:tableurl="tableUrl" v-bind:columheaderseach="columHeaderSeach"></erp-table>

        </div>
    </div>
</div>
@Scripts.Render("~/Areas/BasicData/Content/js/LogisticsCarriageList.js?v=" + ViewData["jsVersion"])

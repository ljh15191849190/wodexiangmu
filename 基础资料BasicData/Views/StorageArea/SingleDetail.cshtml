@{
    Layout = "~/Views/Shared/_Index.cshtml";
}

<div id="app" class="clearfix">
    <erp-text v_id="Storage_Area_Name" v-model="B_Storage_Area.Storage_Area_Name" CheckEmpty="true" CheckLength="100" class="pull-left mb-5">区域名称</erp-text>
    <erp-text v_id="Storage_Area_Short_Name" v-model="B_Storage_Area.Storage_Area_Short_Name" CheckEmpty="true" CheckLength="50" CheckNumberOrEngish="true" class="pull-left mb-5 ml-5" >区域简称</erp-text>
    <erp-search v_id="Storage_Id" v-model="B_Storage_Area.Storage_Id" CheckEmpty="true" v-bind:template_selection="TemplateSelection" v-bind:main_orderby="mainOrderBy"  bind_type="CustomTable" table_name="B_Storage" system_key="BasicData" v-bind:filed_seach="filedSearch" v-bind:filed_result="Storageresult" v-bind:input_field="inputField" isselect="true" class="search pull-left mb-5">仓库名称</erp-search>
    <erp-textarea v_id="Remark" v-model="B_Storage_Area.Remark" CheckLength="200" class="pull-left">备注</erp-textarea>
</div>
@Scripts.Render("~/Areas/BasicData/Content/js/StorageAreaSingleDetail.js?v=" + ViewData["jsVersion"])

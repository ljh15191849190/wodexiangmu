@{
    Layout = "~/Views/Shared/_Index.cshtml";
}
<div id="app" class="pt-5 pl-5 pr-5" v-cloak>
    <div class="row m-0">
        <div class="col-md-12 col-sm-12 col-lg-12 pl-5">
            <div class="tile_body clearfix">
                <form class="form-inline" id="searchForm">
                    <div id="order_2" class="form-group show-order drag_1">
                        <erp-text field_name="P_Purchase_Order.Purchase_Order_Id" query_type="01" expression="01" v-model="P_Purchase_Order.Purchase_Order_Id" v_id="Purchase_Order_Id">定货单号</erp-text>
                    </div>
                    <div id="order_5" class="form-group show-order drag_1">
                        <erp-productrange child="true" child1="true" field_name="P_Purchase_Order_Detail.Product_Name" v-bind:productrange="productrange" v-model="P_Purchase_Order.Product_Name" query_type="01" expression="05" v_id="Product_Name">型号</erp-productrange>
                    </div>
                    <div id="order_3" class="form-group show-order drag_1">
                        <erp-datedb field_name="P_Purchase_Order.Create_Time" v_id="Create_Time" v-model="P_Purchase_Order.Create_Time" query_type="01" expression="05">创建日期</erp-datedb>
                    </div>
                    <div id="order_1" class="form-group show-order drag_1">
                        <erp-multselect field_name="P_Purchase_Order.Purchase_Order_Type" v-model="P_Purchase_Order.Purchase_Order_Type" query_type="01" expression="04" v_id="Purchase_Order_Type" bind_type="DataDictionary" system_key="Purchase" dictionary_key="PURCHASE_ORDER_TYPE">定货单类型</erp-multselect>
                    </div>
                    <div id="order_4" class="form-group show-order drag_1">
                        <erp-searchtext class="search" field_name="B_Supplier.Supplier_Name" query_type="01" expression="01" v_id="Supplier_Name" v-model="P_Purchase_Order.Supplier_Name" control_type="supplier" system_key="Purchase">供应商</erp-searchtext>
                    </div>
                    <div id="order_6" class="form-group show-order drag_1">
                        <erp-searchtext child="true" class="search" field_name="P_Purchase_Order_Detail.Brand_Name" query_type="01" expression="01" v_id="Brand_Name" v-model="P_Purchase_Order.Brand_Name" control_type="brand" system_key="Purchase">品牌</erp-searchtext>
                    </div>
                    <div id="order_7" class="form-group show-order drag_1">
                        <erp-multselect child="true" field_name="P_Purchase_Order_Detail.Product_Area_Id" class="search" child_table="false" v_id="Product_Area_Id" v-model="P_Purchase_Order.Product_Area_Id" query_type="01" expression="04" v-bind:main_orderby="area.mainOrderBy" bind_type="CustomTable" table_name="B_Product_Area" system_key="BasicData" v-bind:filed_seach="area.filedSearch" v-bind:filed_result="area.Menuresult" v-bind:input_field="area.inputField" isselect="true">产地</erp-multselect>
                    </div>
                    <div id="order_8" class="form-group show-order drag_1">
                        <erp-text child="true" field_name="P_Purchase_Order_Detail.Package" query_type="01" expression="01" v-model="P_Purchase_Order.Package" v_id="Package">油脂包装</erp-text>
                    </div>
                    <div id="order_9" class="form-group show-order drag_1">
                        <erp-searchtext child="true" class="search" field_name="P_Purchase_Order_Detail.Product_Classify_Name" query_type="01" expression="01" v-model="P_Purchase_Order.Product_Classify_Name" v_id="Product_Classify_Name" control_type="productBigClassify" system_key="Purchase">品名</erp-searchtext>
                    </div>
                    <div id="order_13" class="form-group show-order drag_1">
                        <erp-multselect field_name="P_Purchase_Order.Approval_Status" query_type="01" expression="04" v_id="Approval_Status" v-model="P_Purchase_Order.Approval_Status" bind_type="StatusDictionary" system_key="Purchase" dictionary_key="APPROVAL_STATUS">审核状态</erp-multselect>
                    </div>
                    <div id="order_10" class="form-group show-order drag_1">
                        <erp-searchtext class="search" field_name="B_Employee.Name" query_type="01" expression="01" v-model="P_Purchase_Order.Employee_Name" v_id="Employee_Name" control_type="employee" system_key="Purchase">任务人</erp-searchtext>
                    </div>
                    <div id="order_12" class="form-group show-order drag_1">
                        <erp-searchtext class="search" field_name="B_Department.Department_Name" query_type="01" expression="01" v-model="P_Purchase_Order.Department_Name" v_id="Department_Name" control_type="department" system_key="Purchase">部门</erp-searchtext>
                    </div>
                    <div id="order_11" class="form-group show-order drag_1">
                        <erp-searchtext class="search" field_name="B_Employee.Name" query_type="01" expression="01" v-model="P_Purchase_Order.Create_Employee_Name" v_id="Create_Employee_Name" control_type="employee" system_key="Purchase">创建人</erp-searchtext>
                    </div>
                </form>
                <div class="submitSearch">
                    <span class="moreSearch">更多<i class="fa fa-angle-double-down"></i></span>
                    <erp-button v-bind:btn_cb="Search" v_id="btnSearch" fa_icon="fa fa-search" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b mb-10">查询</erp-button>
                </div>
            </div>
        </div>
        <div class="col-md-12 col-sm-12 col-lg-12  p-0">
            <erp-table showcheckbox="true" v-bind:pagetotalback="pagetotalback" v-bind:columrender="columrender" v-bind:searchmodel="searchModel" v_id="OrderApplyDetailList" v-bind:tableurl="tableUrl" v-bind:columheaderseach="columHeaderSeach"></erp-table>
        </div>
    </div>
</div>
@Scripts.Render("~/Areas/Purchase/Content/js/BorrowApplicationFromLead.js?v=" + ViewData["jsVersion"])






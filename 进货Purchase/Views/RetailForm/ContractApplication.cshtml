            Search: function (obj) {
                vmMenu.searchModel = $("#searchForm").GetSearchModel({ Menu_Id: "M000001", List_Id: "MenuList" });
            },
            Back:function(){
                window.location.href = PageCommon.SetUrl("Detail");
            }
        }

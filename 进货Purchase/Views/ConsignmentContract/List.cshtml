<style>
    /*收缩图标*/
    .down1 {
        position: absolute;
        right: 10px;
        width: 20px;
        font-size: 3.5em;
        color: #FFFFFF;
        cursor: pointer;
    }

    /*快速分配样式*/
    .fast_allocation {
        height: 28px;
        line-height: 28px;
        margin: 6px 6px 0;
        padding: 0 15px;
        border-radius: 2px;
        font-weight: 400;
        cursor: pointer;
        text-decoration: none;
        color: #fff;
        border-color: #ac2925;
    }

        .fast_allocation:hover {
            background-color: #ac2925;
        }
    /*帮助信息*/
    .helpinfo {
        width: 450px;
        list-style: none;
        position: absolute;
        top: 20px;
        right: 60px;
        background: #fff;
        border: 1px solid #000;
        border-radius: 5px;
    }

        .helpinfo li {
            width: 200px;
            color: red;
        }

    #apps .www .select2-container--default .select2-selection--multiple .select2-selection__rendered {
        overflow: hidden !important;
        height: 100%;
    }

    #apps .www .select2-container--default {
        width: 100% !important;
    }

    .tile-body3 {
        position: relative;
        max-height: 247px;
        overflow: hidden;
    }

    .more {
        cursor: pointer;
    }

    .head_bg th {
        border-right: 1px solid #ddd;
    }
    .dropdown-menu li{
        height:30px;
        line-height:30px;
        cursor:pointer;
        text-align:center;
    }
    /*tfoot-----s*/
    .dataTables_scroll {
        position: relative;
    }
    .dataTables_scrollFoot {
        position: absolute;
        bottom: 15px;
    }
    table.dataTable tfoot {
         background-color:#ebf2fa !important;
    }
    table.dataTable tfoot th{
        border-top:0;
        border-right:1px solid #ddd !important;
    }
 /*tfoot-------e*/

    .down1 {
        position: absolute;
        right: 10px;
        width: 20px;
        font-size: 3.5em;
        color: #FFFFFF;
        cursor: pointer;
    }

    ul, li {
        list-style: none;
    }

    #tab li {
        list-style: none;
        border: 1px solid #7692cb;
        background: #fff;
        width: 100px;
        text-align: center;
        line-height: 35px;
        cursor: pointer;
    }


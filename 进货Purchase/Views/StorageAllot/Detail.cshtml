<style>
    .mask-partcont {
        position: relative;
    }

    .mask {
        box-sizing: border-box;
        margin-right: 30px;
        position: absolute;
        top: 0;
        left: 0;
        width: 87%;
        height: 30px;
        background: rgba(0, 0, 0, 0.2);
    }

    .mask2 {
        width: 100%;
    }

    .mask-partcont-text {
        height: 28px;
        line-height: 28px;
        background: #fff;

@{
    Layout = "~/Views/Shared/_Index.cshtml";
}
<div id="app" class="pt-5 pl-5 pr-5" v-cloak>
    <div class="row m-0">
        <div class="col-md-12 col-sm-12 col-lg-12 pl-5">
            <div class="tile_body clearfix">
                <form class="form-inline" id="searchForm">
                    <div id="order_3" class="form-group show-order drag_1">
                        <erp-productrange field_name="M_Order_Apply_Detail.Product_Name" v-bind:productrange="productrange" v-model="M_Order_Apply.Product_Name" query_type="01" expression="05" v_id="Product_Name">型号</erp-productrange>
                    </div>
                    <div id="order_2" class="form-group show-order drag_1">
                        <erp-text field_name="M_Order_Apply_Detail.Order_Apply_Id" query_type="01" expression="01" v-model="M_Order_Apply.Order_Apply_Id" v_id="Order_Apply_Id">申请单号</erp-text>
                    </div>
                    <div id="order_1" class="form-group show-order drag_1">
                        <erp-datedb field_name="M_Order_Apply_Detail.Create_Time" v_id="Create_Time" v-model="M_Order_Apply.Create_Time" query_type="01" expression="05">制单日期</erp-datedb>
                    </div>
                    <div id="order_4" class="form-group show-order drag_1">
                        <erp-searchtext class="search" field_name="M_Order_Apply_Detail.Brand_Name" query_type="01" expression="01" v_id="Brand_Name" v-model="M_Order_Apply.Brand_Name" control_type="brand" system_key="Purchase">品牌</erp-searchtext>
                    </div>
                    <div id="order_5" class="form-group show-order drag_1">
                        <erp-searchtext class="search" field_name="M_Order_Apply_Detail.Create_Employee_Name" query_type="01" expression="01" v-model="M_Order_Apply.Create_Employee_Name" v_id="Create_Employee_Name" control_type="employee" system_key="Purchase">创建人</erp-searchtext>
                    </div>
                    <div id="order_6" class="form-group show-order drag_1">
                        <erp-searchtext class="search" field_name="M_Order_Apply_Detail.Create_Department_Name" query_type="01" expression="01" v-model="M_Order_Apply.Create_Department_Name" v_id="Create_Department_Name" control_type="department" system_key="Purchase">部门</erp-searchtext>
                    </div>
                </form>
            <div class="submitSearch">
                <span class="moreSearch">更多<i class="fa fa-angle-double-down"></i></span>
                <erp-button v-bind:btn_cb="Search" v_id="btnSearch" fa_icon="fa fa-search" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b mb-10">查询</erp-button>
            </div>
        </div>
        </div>
        <div class="col-md-12 col-sm-12 col-lg-12  p-0">
            <erp-table multiselect="true" showcheckbox="true" v-bind:searchmodel="searchModel" v-bind:columrender="columrender" v_id="OrderApplyDetailList" v-bind:tableurl="tableUrl" v-bind:columheaderseach="columHeaderSeach"></erp-table>
        </div>
    </div>
</div>
@Scripts.Render("~/Areas/Purchase/Content/js/OrderFormSalesContract.js?v=" + ViewData["jsVersion"])





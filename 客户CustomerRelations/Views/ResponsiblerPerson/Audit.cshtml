            placeholder: "请选择",
            minimumResultsForSearch: -1,
            templateSelection: function formatRepoSelection(repo) {
                $("#ModelNumber,#brand,#customerName,#Task,#department,#AduitMark,#typeSpecification2,#receiveTask").val("");
                return $(repo.element).attr("value");
            }
        });

        $("#paymentWay").select2({
            minimumResultsForSearch: Infinity
        });
        $("#paymentWay1,#bills,#currency,#apping").select2({
            placeholder: "请选择",
            allowClear: true,//删除x图标
            minimumResultsForSearch: Infinity
        });


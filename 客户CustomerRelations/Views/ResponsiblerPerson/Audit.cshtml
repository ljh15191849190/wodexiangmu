<script>
    $(document).ready(function () {
        $('#apps').click(function () {
            $('.provider_detailed').hide();
            $('.type_detailed').hide();
            $('.client_detailed').hide();
        })
        $(".provider_detailed .provider_tabs").on('click', 'li', function (e) {
            $(this).addClass('active').siblings().removeClass('active');
            var index = $(this).index();
            $(".provider_detailed .information_list").find('div').eq(index).show().siblings().hide();
            PageCommon.RefreshTablePage("orderForm6");
            e.stopPropagation();
        })
        $(".type_detailed .type_tabs").on('click', 'li', function (e) {
            $(this).addClass('active').siblings().removeClass('active');
            var index = $(this).index();
            $(".type_detailed .information_list").find('div').eq(index).show().siblings().hide();
            PageCommon.RefreshTablePage("orderForm5");
            e.stopPropagation();
        })
        $(".client_detailed .client_tabs").on('click', 'li', function (e) {
            $(this).addClass('active').siblings().removeClass('active');
            var index = $(this).index();
            $(".client_detailed .information_list").find('div').eq(index).show().siblings().hide();
            PageCommon.RefreshTablePage("orderForm7");
            e.stopPropagation();
        })
        $("#tab").on("click", "li", function () {
            var index = $(this).index();

            $(this).addClass("tabactive");
            $(this).siblings().removeClass("tabactive");
            $("#tables1 .table").hide();
            $("#tables1 .table").eq(index).show();
        })

        $(".help").click(function () {
            $(".helpinfo").toggle();
        })

        $(".down1").click(function () {
            $(".searchForm_Detail").toggle();
            $(this).find("i").toggleClass('rotate');

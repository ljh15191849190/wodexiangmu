<style>
    /*小标题*/
    .title1 {
        height: 30px;
        width: 200px;
        background-image: url('../../../../Content/assets/images/title.png');
        background-size: 125px 30px;
        background-position: left bottom;
        background-repeat: no-repeat;
        margin-bottom: 5px;
        margin-top: 5px;
        line-height: 30px;
        text-indent: 20px;
        font-size: 2.2em;
        color: #fff;   
    }
    /*jquery-ui-bootstrap tabs*/
    .tabs > ul {
        font-size: 13px !important;
        border-bottom: 2px solid #39aef5 !important;
    }
        /*.tabs ul li:not(:first-child){
        padding-right:15px!important;
    }*/
        .tabs > ul li {
            padding-right: 12px !important;
        }

        .tabs > ul li {
            border-top: 1px solid #ccc !important;
            border-left: 1px solid #ccc !important;
            display: inline-block;
        }

            .tabs > ul li a {
                color: #666 !important;
                display: inline-block;
                padding: 3px 5px;
            }

            .tabs > ul li span:hover {
                color: #C61010 !important;
            }

            .tabs > ul li a:hover,
            .tabs > ul li:hover,
            .tabs > ul li:focus {
                border-bottom: 0 !important;
            }

            .tabs > ul li:last-child {
                border-right: 1px solid #ccc !important;
            }

            .tabs > ul li.ui-state-active.ui-tabs-active a,
            .tabs > ul li.ui-state-active.ui-tabs-active span,
            .tabs > ul li.ui-state-active.ui-tabs-active {
                /*border-top:3px solid red!important;*/
                background: #39aef5 !important;
                /*border-bottom: 2px solid #39aef5!important;*/
                color: #ddd !important;
            }

                .tabs > ul li.ui-state-active.ui-tabs-active a:hover,
                .tabs > ul li.ui-state-active.ui-tabs-active span:hover {
                    color: #fff !important;
                }

    .financialleft {
        height: 500px;
        border: 1px solid #ddd;
    }

        .financialleft p {
            background: #39aef5;
        }

    .menu {
        margin: 0 auto;
    }
        /*.menu li{
        width:108px;
    }*/
        .menu a, .financialleft p {
            display: block;
            border: 1px solid #41bede;
            /*width:108px;*/
            text-align: center;
            height: 30px;
            line-height: 30px;
            border-bottom: none;
            font-size: 13px;
            color: #000;
        }

        .menu li:last-child a {
            border-bottom: 1px solid #41bede;
        }
        .BasicInformation, .PayInformation {
        width: 100%;
        height: auto;
        background-color: #eff2f5;
        box-shadow: 0px 0px 2px #9a9696;
        padding: 5px;
        border: 1px solid #abb3b9;
    }
    .boder {
        margin-top: 5px;
        box-shadow: 1px 0px 2px #9a9696;
        border: 1px solid #abb3b9;
        background-color: #eff2f5;
        border-left: 0px;
    }
    .mask-partcont {
        position: relative;
    }

    .mask {
        box-sizing: border-box;
        margin-right: 30px;
        position: absolute;
        top: 0;
        left: 0;
        width: 87%;
        height: 30px;
        background: rgba(0, 0, 0, 0.2);
    }

    .mask2 {
        width: 100%;
    }

    .mask-partcont-text {
        height: 28px;
        line-height: 28px;
        background: #fff;
        width: 100%;

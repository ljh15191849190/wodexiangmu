                }
            })
        })
        $('#apps').click(function () {
            $('.client_detailed').hide();
            $('.consignee_detailed').hide();
        })

        $(".client_detailed .client_tabs").on('click', 'li', function (e) {
            $(this).addClass('active').siblings().removeClass('active');
            var index = $(this).index();
            $(".client_detailed .information_list").find('div').eq(index).show().siblings().hide();
            PageCommon.RefreshTablePage("orderForm7");
            e.stopPropagation();
        })
        var table7 = $('#orderForm7').DataTable({
            "select": true,
            "autoWidth": false,
            "sScrollX": true,
            "sScrollXInner": 800,
            "dom": 'rt',
            "searching": false,
            "sScrollY": true,
            "paging": false//不分页
        });
        PageCommon.RefreshTablePage("orderForm7");
        $(table7.table().container()).width(440);

        $(".down1").click(function () {
            $("#searchForm_detail").toggle();
            $(this).find("i").toggleClass('rotate');
            $("#div_table_orderForm .dataTables_scrollBody").height($(window).outerHeight() - $("#div_table_orderForm .dataTables_scrollBody").offset().top - $("#div_table_orderForm .tableBottom").outerHeight());
            $("#div_table_orderForm3 .dataTables_scrollBody").height($(window).outerHeight() - $("#div_table_orderForm3 .dataTables_scrollBody").offset().top - $("#div_table_orderForm3 .tableBottom").outerHeight());
            PageCommon.RefreshTablePage("orderForm");
            PageCommon.RefreshTablePage("orderForm3");
        })
        $("#provider2,#typeSpecification2,#prepared").select2({
            placeholder: "请选择",
            minimumResultsForSearch: -1,
            templateSelection: function formatRepoSelection(repo) {
                $("#provider2,#typeSpecification2").val("");
                return $(repo.element).attr("value");
            }
        });
        $("#apping,#paymentWay3,#paymentWay4,#paymentWay5,#paymentWay6,#typeSpecification3").select2({
            placeholder: "请选择",
            allowClear: true,//删除x图标
            minimumResultsForSearch: Infinity
        });
        $("#typeSpecification2,#typeSpecification3,#prepared").select2({
            placeholder: "请选择",
            minimumResultsForSearch: -1,
            templateSelection: function formatRepoSelection(repo) {
                $("#typeSpecification2,#typeSpecification3,#prepared").val("");
                return $(repo.element).attr("value");
            }
        });

        $("#paymentWay2,#bills5,#bills6,#bills1").select2({
            placeholder: "请选择",
            allowClear: true,//删除x图标
            minimumResultsForSearch: Infinity
        });

        var table = $('#orderForm').DataTable({
            "select": true,
            "autoWidth": false,
            "sScrollX": true,
            "sScrollXInner": 2000,
            "dom": 'rt',
            "searching": false,
            "sScrollY": true,
            "paging": false,//不分页
            //fixedColumns: true,
            //select: { style: 'multi' },
            keys: {
                columns: [13,16,17]
            },
            "language": {
                "sProcessing": "处理中...",
                "sLengthMenu": "显示 _MENU_ 项结果",
                "sZeroRecords": "没有匹配结果",
                "sInfo": "显示第 _START_ 至 _END_ 条结果，共 _TOTAL_ 条",
                "sInfoEmpty": "显示第 0 至 0 项结果，共 0 项",
                "sInfoFiltered": "(由 _MAX_ 项结果过滤)",
                "sInfoPostFix": "",
                "sSearch": "搜索:",
                "sUrl": "",
                "sEmptyTable": "查询的数据不存在！",
                "sLoadingRecords": "载入中...",
                "sInfoThousands": ",",
                "oPaginate": {
                    "sFirst": "首页",
                    "sPrevious": "上页",
                    "sNext": "下页",
                    "sLast": "末页"
                },
                "oAria": {
                    "sSortAscending": ": 以升序排列此列",
                    "sSortDescending": ": 以降序排列此列"
                }
            }
        });

        $("div.toolbar").html("<div style='font-size:12px;color:#333;padding-top:3px;'>跳转到第<input type='text' style='width:30px;height:20px;margin:0 5px 0 5px;'/>页<span style='color:#fff;font-size:12px;cursor: pointer;margin-left:10px;padding:2px 3px;' class='btn-primary'>跳转</span><div>")
        $("#div_table_orderForm .dataTables_scrollBody").height($(window).outerHeight() - $("#div_table_orderForm .dataTables_scrollBody").offset().top - $("#div_table_orderForm .tableBottom").outerHeight());
        PageCommon.RefreshTablePage("orderForm");

        table.on('key-focus', function (e, datatable, cell, originalEvent) {
            //if (cell.index().column == 0 || cell.index().column == 1) {
            //    return;
            //}
            //var titleModel = columntitle[cell.index().column];
            var rowIndex = cell.index().row;
            var rowData = datatable.row(rowIndex).data();

            menu_vue.rowEditCallback(cell, cell.index().column, rowData);
        });

        table.on('key-blur', function (e, datatable, cell, originalEvent) {
            //if (cell.index().column == 0 || cell.index().column == 1) {
            //    return;
            //}
            //var titleModel = vm.columntitle[cell.index().column];
            var rowIndex = cell.index().row;
            var rowData = datatable.row(rowIndex).data();
            menu_vue.rowEditBlurCallback(cell, datatable, cell.index().column, rowData);
        });

        $("#orderForm tbody tr").dblclick(function () {
            PageCommon.ModalOpen({
                id: "Contract",
                title: '盘点计划查询',
                width: "900px",
                height: "600px",
                btn: ['添加'],
                url: '/Warehouse/CheckStock/CheckStockSearch',
                callBack: function (iframeId) {
                    PageCommon.ChildrenFrames(iframeId).codeset_vue.Save()
                }
            })
        })
    });
</script>

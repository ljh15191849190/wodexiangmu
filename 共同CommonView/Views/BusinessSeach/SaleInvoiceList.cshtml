@{
    Layout = "~/Views/Shared/_Index.cshtml";
}
<div id="app" class="pt-5 pl-5 pr-5 SaleInvoice" v-cloak>
    <div class="row m-0">
        <div class="col-md-12 col-sm-12 col-lg-12 pl-5">
            <div class="tile_body searchHide clearfix">
                <form class="form-inline" id="searchForm">
                    <div id="order_1" class="form-group show-order">
                        <erp-productrange child="true" child1="true" v-bind:system_key="searchModel.Pagination_Info.System_Key" field_name="F_Sale_Invoice_Detail.Product_Name" v-bind:productrange="productrange" query_type="01" expression="05" v_id="productrange">型号</erp-productrange>
                    </div>
                    <div id="order_5" class="form-group show-order">
                        <erp-datedb field_name="F_Sale_Invoice.Invoice_Date" v_id="Invoice_Date" v-model="Invoice_Date" query_type="03" expression="05">开票日期</erp-datedb>
                    </div>
                    <div id="order_12" class="form-group show-order drag_1">
                        <erp-searchtext class="search" field_name="F_Sale_Invoice.Create_Department_Name" query_type="01" expression="01" v-model="Create_Department_Name" v_id="Create_Department_Name" control_type="department" v-bind:system_key="searchModel.Pagination_Info.System_Key">创建部门</erp-searchtext>
                    </div>
                    <div id="order_11" class="form-group show-order drag_1">
                        <erp-searchtext class="search" field_name="F_Sale_Invoice.Create_Employee_Name" query_type="01" expression="01" v-model="Create_Employee_Name" v_id="Create_Employee_Name" control_type="employee" v-bind:system_key="searchModel.Pagination_Info.System_Key">创建员工</erp-searchtext>
                    </div>
                    <div id="order_11" class="form-group show-order drag_1">
                        <erp-searchtext class="search" field_name="F_Sale_Invoice.Customer_Name" query_type="01" expression="01" v-model="Customer_Name" v_id="Customer_Name" control_type="customer" v-bind:system_key="searchModel.Pagination_Info.System_Key">客户</erp-searchtext>
                    </div>
                    <div id="order_11" class="form-group show-order drag_1">
                        <erp-searchtext class="search" field_name="F_Sale_Invoice.Task_Employee_Name" query_type="01" expression="01" v-model="Task_Employee_Name" v_id="Task_Employee_Name" control_type="employee" v-bind:system_key="searchModel.Pagination_Info.System_Key">任务人</erp-searchtext>
                    </div>
                    <div id="order_11" class="form-group show-order drag_1">
                        <erp-searchtext class="search" field_name="F_Sale_Invoice.Task_Department_Name" query_type="01" expression="01" v-model="Task_Department_Name" v_id="Task_Department_Name" control_type="department" v-bind:system_key="searchModel.Pagination_Info.System_Key">任务人部门</erp-searchtext>
                    </div>
                    <div id="order_2" class="form-group show-order">
                        <erp-text field_name="F_Sale_Invoice.Invoice_Number" query_type="01" expression="01" v-model="Invoice_Number" v_id="Invoice_Number">发票号</erp-text>
                    </div>
                    <div id="order_2" class="form-group show-order">
                        <erp-multselect field_name="F_Sale_Invoice.Invoice_Status" query_type="01" expression="04" v_id="Invoice_Status" v-model="Invoice_Status" bind_type="StatusDictionary" dictionary_key="INVOICE_STATUS" v-bind:system_key="searchModel.Pagination_Info.System_Key">发票状态</erp-multselect>
                    </div>
                </form>
                <div class="submitSearch">
                    <span class="moreSearch">更多<i class="fa fa-angle-double-down"></i></span>
                    <erp-button v-bind:btn_cb="Search" v_id="btnSearch" fa_icon="fa fa-search" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b mb-10">查询</erp-button>
                    <erp-button v-bind:btn_cb="mClear" v_id="btnClear" fa_icon="fa fa-times" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b mb-10">清空</erp-button>
                </div>
            </div>
        </div>
        <div class="col-md-12 col-sm-12 col-lg-12  p-0">
            <erp-table isinfo="false" v-bind:pagetotalback="pagetotalback" v-bind:childtotalback="childtotalback" v-bind:columrender="columrender" v-bind:childcolumrender="childcolumrender" ischild1tfootshow="true" istfootshow="true" v-bind:childsearchkey="childSearchModel" showcheckbox="true" v-bind:searchmodel="searchModel" v_id="SaleInvoiceList" v-bind:tableurl="tableUrl" v-bind:columheaderseach="columHeaderSeach"></erp-table>
        </div>
    </div>
</div>
@Scripts.Render("~/Areas/CommonView/Content/js/SaleInvoiceList.js?v=" + ViewData["jsVersion"])





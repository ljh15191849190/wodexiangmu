                    <erp-textarea v-model="receiver_Seach.Customer_Send_Invoice.Apply_Reason" v_id="Apply_Reason" CheckLength="500">申请理由</erp-textarea>
                </div>
                <div id="order_7" class="form-group show-order base">
                    <erp-textarea v-model="receiver_Seach.Customer_Send_Invoice.Remark" v_id="Remark" CheckLength="500">备注</erp-textarea>
                </div>
                <div id="order_7" class="form-group show-order base">
                    <erp-switch v_id="Is_Default" v-model="receiver_Seach.Customer_Send_Invoice.Is_Default">是否默认</erp-switch>

                </div>
                <div id="order_7" class="form-group show-order base">
                    <erp-text v-model="receiver_Seach.Supplier_Receiver.Mobile_Phone" v_id="Mobile_Phone" CheckMobilPhone="true">手机</erp-text>
                </div>
                <div id="order_7" class="form-group show-order base">
                    <erp-text v-model="receiver_Seach.Supplier_Receiver.Email" v_id="Email" CheckLength="50">邮箱</erp-text>
                </div>
                <div id="order_7" class="form-group show-order base">
                    <erp-address v-bind:address="addressCommon" v_id="Address" CheckEmpty="true" CheckLength="200">地址</erp-address>
                </div>
                <div id="order_7" class="form-group show-order base">
                    <erp-text v-model="receiver_Seach.Supplier_Receiver.Fax" v_id="Fax" CheckLength="15">传真</erp-text>
                </div>
                <div id="order_7" class="form-group show-order base">

                    <erp-text v-model="receiver_Seach.Supplier_Receiver.Wechat" v_id="Wechat" CheckLength="30">微信</erp-text>
                </div>
                <div id="order_7" class="form-group show-order base">
                    <erp-text v-model="receiver_Seach.Supplier_Receiver.Post_Code" v_id="Post_Code" CheckEmpty="true" CheckLength="6">邮政编码</erp-text>
                </div>
                <div id="order_7" class="form-group show-order base">
                    <erp-text v-model="receiver_Seach.Supplier_Receiver.Department_Name" v_id="Department_Name" CheckEmpty="true">部门名称</erp-text>
                </div>
                <div id="order_7" class="form-group show-order base">
                    <erp-switch v_id="Is_Default" v-model="receiver_Seach.Supplier_Receiver.Is_Default">是否默认</erp-switch>
                </div>
            </form>
            <form v-if="receivertype === '2'" class="form-inline pl-0" id="searchForm_detail">
                <div id="order_7" class="form-group show-order base">
                    <erp-text v-model="receiver_Seach.Customer_Send_Invoice.Name" v_id="Name" CheckEmpty="true" CheckLength="30">姓名</erp-text>
                </div>
                <div id="order_7" class="form-group show-order base">
                    <erp-text v-model="receiver_Seach.Supplier_Receiver.Tel" v_id="Tel" CheckTelephone="true" CheckEmptyOneBetweenTwo="true" RelationControl="Mobile_Phone">联系电话</erp-text>
                </div>
                <div id="order_7" class="form-group show-order base">
                    <erp-text v-model="receiver_Seach.Supplier_Receiver.Mobile_Phone" v_id="Mobile_Phone" CheckMobilPhone="true">手机</erp-text>
                </div>
                <div id="order_7" class="form-group show-order base">
                    <erp-text v-model="receiver_Seach.Customer_Send_Invoice.Post_Code" v_id="Post_Code" CheckEmpty="true" CheckLength="6">邮政编码</erp-text>
                </div>
                <div id="order_7" class="form-group show-order base">
                    <erp-address v-bind:address="addressCommon" v_id="Address" CheckEmpty="true" CheckLength="200">寄票地址</erp-address>
                </div>

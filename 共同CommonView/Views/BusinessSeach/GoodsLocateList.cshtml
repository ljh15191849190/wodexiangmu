@{
    Layout = "~/Views/Shared/_Index.cshtml";
}
<div id="app" class="pt-5 pl-5 pr-5 CustomerSearch" v-cloak>
    <div class="row m-0">
        <div class="col-md-12 col-sm-12 col-lg-12 pl-5">
            <div class="tile_body searchHide clearfix">
                <form class="form-inline" id="searchForm">
                    <div id="order_1" class="form-group show-order">
                        <erp-text field_name="B_Goods_Locate.Goods_Locate_Name" query_type="01" expression="01" v-model="Goods_Locate_Name" v_id="Goods_Locate_Name">货位名称</erp-text>
                    </div>
                    <div id="order_2" class="form-group show-order">
                        <erp-multselect field_name="B_Goods_Locate.Goods_Locate_Type" query_type="01" expression="04" v_id="Goods_Locate_Type" v-model="Goods_Locate_Type" bind_type="DataDictionary" v-bind:system_key="searchModel.Pagination_Info.System_Key" v-bind:dictionary_key="GoodsLocateTypeDictionaryKey">货位类型</erp-multselect>
                    </div>
                    <div id="order_2" class="form-group show-order">
                        <erp-searchtext class="search" field_name="B_Storeage.Storeage_Name" query_type="01" expression="01" v-model="Storeage_Name" v_id="Storeage_Name" control_type="storeage" v-bind:system_key="searchModel.Pagination_Info.System_Key">仓库</erp-searchtext>
                    </div>
                    <div id="order_2" class="form-group show-order">
                        <erp-multselect field_name="B_Goods_Locate.Storeage_Area_Id" query_type="01" expression="04" v_id="Storeage_Area_Id" v-model="Storeage_Area_Id" bind_type="CustomTable" table_name="B_Storeage_Area" v-bind:system_key="searchModel.Pagination_Info.System_Key" v-bind:main_where="StoreageAreaWhere" v-bind:main_orderby="StoreageAreaOrder" v-bind:Filed_Result="StoreageAreaResult">仓库区域</erp-multselect>
                    </div>
                    <div id="order_3" class="form-group show-order">
                        <erp-multselect field_name="B_Goods_Locate.Storeage_Region_Id" query_type="01" expression="04" v_id="Storeage_Region_Id" v-model="Storeage_Region_Id" bind_type="CustomTable" table_name="B_Storeage_Region" v-bind:system_key="searchModel.Pagination_Info.System_Key" v-bind:main_where="StoreageRegionWhere" v-bind:main_orderby="StoreageRegionOrder" v-bind:Filed_Result="StoreageRegionResult">仓库储区</erp-multselect>
                    </div>
                    <div id="order_4" class="form-group show-order">
                        <erp-datedb field_name="B_Goods_Locate.Create_Time" v_id="Create_Time" v-model="Create_Time" query_type="01" expression="05">创建时间</erp-datedb>
                    </div>
                </form>
                <div class="submitSearch">
                    <span class="moreSearch">更多<i class="fa fa-angle-double-down"></i></span>
                    <erp-button v-bind:btn_cb="Search" v_id="btnSearch" fa_icon="fa fa-search" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b mb-10">查询</erp-button>
                    <erp-button v-bind:btn_cb="mClear" v_id="btnClear" fa_icon="fa fa-times" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b mb-10">清空</erp-button>
                </div>
            </div>
        </div>
        <div class="col-md-12 col-sm-12 col-lg-12 p-0">
            <erp-table isinfo="false" showcheckbox="true" v-bind:searchmodel="searchModel" v_id="GoodsLocateList" v-bind:tableurl="tableUrl" v-bind:columheaderseach="columHeaderSeach"></erp-table>
        </div>
    </div>
</div>
@Scripts.Render("~/Areas/CommonView/Content/js/GoodsLocateList.js?v=" + ViewData["jsVersion"])

@{
    Layout = "~/Views/Shared/_Index.cshtml";
}
<div id="apps" class="pt-5 pl-5 pr-5">
    <div class="row m-0">
        <div class="col-md-12 col-sm-12 row-tit p-0 mb-5">
            <div id="T_Menu_Alias" class="col-md-12 col-sm-12 p-tit">附件列表</div>
            <div class="down"><i class="fa fa-angle-double-down"></i></div>
        </div>
        <div class="col-md-12 col-sm-12">
            <div class="tile-body2 clearfix">
                <form class="clearfix" id="searchForm">
                    <div id="order_1" class="form-group pull-left">
                        <erp-text v_id="Attachment_Name" v-model="Attachment_Name" field_name="S_Attachment.Attachment_Name" query_type="01" expression="01">附件名称</erp-text>
                    </div>
                    <div id="order_2" class="form-group pull-left show-order">
                        <erp-datedb v_id="Create_Time" v-model="Create_Time" field_name="S_Attachment.Create_Time" query_type="03" expression="05">上传日期</erp-datedb>
                    </div>
                </form>
                <div class="submitSearch">
                    <erp-button v-bind:btn_cb="Search" v_id="btnSearch" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b mb-10" fa_icon="fa fa-search">查询</erp-button>
                </div>
            </div>
        </div>
        <div class="col-md-12 col-sm-12 p-0">
            <erp-table multiselect="true" showcheckbox="true" v-bind:searchmodel="searchModel" v_id="AttachmentList" v-bind:tableurl="tableurl" v-bind:columheaderseach="ColumHeaderSeach"></erp-table>
        </div>
    </div>
</div>
@Scripts.Render("~/Areas/SystemSet/Content/js/Attachment.js?v=" + ViewData["jsVersion"])
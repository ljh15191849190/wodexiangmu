@{
    Layout = "~/Views/Shared/_Index.cshtml";
}
<div id="app" class="clearfix">
    <erp-text v_id="Top_Menu_Name" v-model="S_Top_Menu.Top_Menu_Name" CheckEmpty="true" CheckLength="200" class="pull-left mb-5 mr-5">工具栏名称</erp-text>
    <erp-text v_id="Top_Menu_Image" v-model="S_Top_Menu.Top_Menu_Image" CheckEmpty="true" class="pull-left mb-5 mr-5">工具栏图片</erp-text>
    <erp-number v_id="Show_Order" v-model="S_Top_Menu.Show_Order" CheckEmpty="true" class="pull-left mb-5 mr-5">表示顺</erp-number>
    <erp-switch v_id="Is_Activate" v-model="S_Top_Menu.Is_Activate" class="pull-left mb-5 mr-5">启用</erp-switch>
    <erp-textarea v_id="Remark" v-model="S_Top_Menu.Remark" CheckLength="200" class="pull-left mb-5 mr-5">备注</erp-textarea>
</div>
@Scripts.Render("~/Areas/SystemSet/Content/js/TopMenuDetail.js?v=" + ViewData["jsVersion"])

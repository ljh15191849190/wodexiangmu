@{
    Layout = "~/Views/Shared/_Index.cshtml";
}
<div id="apps" class="pt-5 pl-5 pr-5">
    <div class="row m-0">
        <div class="col-md-12 col-sm-12 col-lg-12 row-tit p-0 mb-5">
            <div id="T_Menu_Alias" class="col-md-4 col-sm-4 col-lg-4 p-tit">状态字典详细列表</div>
            <div class="col-md-8 col-sm-8 col-lg-8 p-btn pr-40">
                <erp-button v-bind:btn_cb="Save" v_id="btnSave" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b" fa_icon="fa fa-plus">保存</erp-button>
                <erp-button v-bind:btn_cb="Back" v_id="btnBack" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b" fa_icon="fa fa-mail-reply">返回</erp-button>
            </div>
            <div class="tit_down"><i class="fa fa-angle-double-up"></i></div>
        </div>
        <div class="col-md-12 col-sm-12 col-lg-12 pl-5">
            <form class="tile_body form-inline" id="searchForm_detail">
                <div id="order_1" class="form-group">
                    <erp-text v-bind:isshade_bg="shade" v_id="Object_Type" v-model="S_Status_Dictionary.Object_Type" CheckEmpty="true">对象类型</erp-text>
                </div>
                <div id="order_2" class="form-group show-order">
                    <erp-text v_id="Group_Name" v-model="S_Status_Dictionary.Group_Name" CheckEmpty="true">对象名称</erp-text>
                </div>
            </form>
        </div>
        <div class="col-md-12 col-sm-12 col-lg-12 p-0">
            <erp-edit-table v-bind:columrender="columrender" v-model="S_Status_Dictionary.S_Status_Dictionary_List" v_id="StatusDictionaryList" v-bind:row_edit_callback="rowEditCallback" v-bind:row_edit_blur_callback="rowEditBlurCallback" v-bind:columheaderseach="columHeaderSeach"></erp-edit-table>
        </div>
    </div>
</div>

@*编码*@
<script type="text/x-handlebars-template" id="temp_Code">
    <div id="div_temp_Code">
        <erp-text v-model="Code" v_id="Code" CheckEmpty="true" CheckLength="200"></erp-text>
    </div>
</script>

@*名称*@
<script type="text/x-handlebars-template" id="temp_Name">
    <div id="div_temp_Name">
        <erp-text v-model="Name" v_id="Name" CheckEmpty="true" CheckLength="200"></erp-text>
    </div>
</script>
@*表示顺*@
<script type="text/x-handlebars-template" id="temp_Show_Order">
    <div id="div_temp_Show_Order">
        <erp-number v-model="Show_Order" v_id="Show_Order" CheckEmpty="true" CheckLength="200"></erp-number>
    </div>
</script>
@*是否激活*@
<script type="text/x-handlebars-template" id="temp_Is_Activate">
    <div id="div_temp_Is_Activate">
        <erp-switch v_id="Is_Activate" v-model="Is_Activate"></erp-switch>
    </div>
</script>

@*备注*@
<script type="text/x-handlebars-template" id="temp_Remark">
    <div id="div_temp_Remark">
        <erp-textarea v_id="Remark" v-model="Remark" CheckLength="500"></erp-textarea>
    </div>
</script>
@Scripts.Render("~/Areas/SystemSet/Content/js/StatusDictionaryDetail.js?v=" + ViewData["jsVersion"])
@{
    Layout = "~/Views/Shared/_Index.cshtml";
}
<div id="apps" class="pt-5 pl-5 pr-5">
    <div class="row m-0">
        <div class="col-md-12 col-sm-12 col-lg-12 row-tit p-0 mb-5">
            <div id="T_Menu_Alias" class="col-md-4 col-sm-4 col-lg-4 p-tit">菜单列表 </div>
            <div class="col-md-8 col-sm-8 col-lg-8 p-btn pr-40">
                <erp-button v-bind:btn_cb="New" v_id="btnNew" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b" fa_icon="fa fa-plus">新增</erp-button>
                <erp-button v-bind:btn_cb="Update" v_id="btnUpdate" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b" fa_icon="fa fa-edit">修改</erp-button>
                <erp-button v-bind:btn_cb="Delete" v_id="btnDelete" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b" fa_icon="fa fa-trash">删除</erp-button>
                <erp-button v-bind:btn_cb="Import" v_id="btnImport" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b" fa_icon="glyphicon glyphicon-import">导入</erp-button>
                <erp-button v-bind:btn_cb="Export" v_id="btnExport" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b" fa_icon="glyphicon glyphicon-export">导出</erp-button>
                <erp-button v-bind:btn_cb="Enable" v_id="btnEnable" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b" fa_icon="fa fa-check-circle">启用</erp-button>
                <erp-button v-bind:btn_cb="Dissable" v_id="btnDissable" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b" fa_icon="fa fa-exclamation-circle">禁用</erp-button>
                <erp-button v-bind:btn_cb="Print" v_id="btnPrint" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b" fa_icon="glyphicon glyphicon-print ">打印</erp-button>
                <erp-button v-bind:btn_cb="Set" v_id="btnSet" btn_opt="设置" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b" fa_icon="fa fa-cog">设置</erp-button>
            </div>
            <div class="tit_down"><i class="fa fa-angle-double-up"></i></div>
        </div>
        <div class="col-md-12 col-sm-12 col-lg-12 pl-5">
            <div class="tile_body searchHide clearfix">
                <form class="form-inline" id="searchForm">
                    <div id="order_1" class="form-group">
                        <erp-select ismustselect="true" field_name="S_Menu.System_Id" query_type="01" expression="02" v-model="System_Name" bind_type="CustomTable" table_name="S_System" system_key="SystemSet" v-bind:main_where="systemWhere" v-bind:main_orderby="systemOrder" v-bind:Filed_Result="systemResult" v_id="System_Name">系统名称</erp-select>
                    </div>
                    <div id="order_2" class="form-group show-order">
                        <erp-multselect field_name="S_Menu.Menu_Group_Id" query_type="01" expression="04" v_id="Menu_Group_Name" v-model="Menu_Group_Name" bind_type="CustomTable" table_name="S_Menu_Group" system_key="SystemSet" v-bind:main_where="menuGroupWhere" v-bind:main_orderby="menuGroupOrder" v-bind:Filed_Result="menuGroupResult">菜单分组</erp-multselect>
                    </div>
                    <div id="order_3" class="form-group show-order">
                        <erp-text field_name="S_Menu.Menu_Name" query_type="01" expression="01" v-model="Menu_Name" v_id="Menu_Name">菜单名称</erp-text>
                    </div>
                    <div id="order_4" class="form-group show-order">
                        <erp-text field_name="S_Menu.Menu_Address" query_type="01" expression="01" v-model="Menu_Address" v_id="Menu_Address">菜单地址</erp-text>
                    </div>
                    <div id="order_5" class="form-group show-order">
                        <erp-text child="true" field_name="S_Button.Button_Name" query_type="01" expression="01" v-model="Button_Name" v_id="Button_Name">按钮名称</erp-text>
                    </div>
                    <div id="order_6" class="form-group show-order">
                        <erp-select ismustselect="true" field_name="S_Menu.Is_Activate" query_type="01" expression="02" v-model="Is_Activate" bind_type="DataDictionary" system_key="SystemSet" v-bind:dictionary_key="IsActiveDictionaryKey" v_id="Is_Activate">启用</erp-select>
                    </div>
                </form>
                <div class="submitSearch">
                    <span class="moreSearch">更多<i class="fa fa-angle-double-down"></i></span>
                    <erp-button v-bind:btn_cb="Search" v_id="btnSearch" fa_icon="fa fa-search" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b mb-10">查询</erp-button>
                    <erp-button v-bind:btn_cb="mClear" v_id="btnClear" fa_icon="fa fa-times" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b mb-10">清空</erp-button>
                </div>
            </div>
        </div>
        <div class="col-md-12 col-sm-12 col-lg-12 p-0">
            <erp-table v-bind:initcomplete="initcomplete" istfootShow="true" ischild1tfootshow="true" v-bind:childsearchkey="childSearchModel" multiselect="true" showcheckbox="true" v-bind:searchmodel="searchModel" v_id="MenuList" v-bind:tableurl="tableUrl" v-bind:columheaderseach="columHeaderSeach"></erp-table>
        </div>
    </div>
</div>
@*按钮名称*@
<script type="text/x-handlebars-template" id="temp_searchtext">
    <div id="searchtext">
        <erp-text v-model="vmodel" v_id="Menu_Alias" CheckLength="200"></erp-text>
    </div>
</script>
@Scripts.Render("~/Areas/SystemSet/Content/js/MenuList.js?v=" + ViewData["jsVersion"])

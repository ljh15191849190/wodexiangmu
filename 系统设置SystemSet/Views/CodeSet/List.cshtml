@{
    Layout = "~/Views/Shared/_Index.cshtml";
}
<div id="apps" class="pt-5 pl-5 pr-5">
    <div class="row m-0">
        <div class="col-md-12 col-sm-12 row-tit p-0 mb-5">
            <div id="T_Menu_Alias" class="col-md-2 col-sm-2 p-tit">编码设置列表</div>
            <div class="col-md-10 col-sm-10 p-btn pr-40">
                <erp-button v-bind:btn_cb="New" v_id="btnNew" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b" fa_icon="fa fa-plus">新增</erp-button>
                <erp-button v-bind:btn_cb="Update" v_id="btnUpdate" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b" fa_icon="fa fa-edit">修改</erp-button>
                <erp-button v-bind:btn_cb="Delete" v_id="btnDelete" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b" fa_icon="fa fa-trash">删除</erp-button>
                <erp-button v-bind:btn_cb="Set" v_id="btnSet" btn_opt="设置" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b" fa_icon="fa fa-cog">设置</erp-button>
            </div>
            <div class="down"><i class="fa fa-angle-double-down"></i></div>
        </div>
        <div class="col-md-12 col-sm-12">
            <div class="tile-body2 clearfix">
                <form class="form-inline" id="searchForm">
                    <div id="order_1" class="form-group">
                        <erp-search class="search" v_id="Table_Name" v-model="Table_Name" field_name="S_Code_Set.Table_Name" ismustselect="true" query_type="01" expression="02" v-bind:template_selection="TemplateSelection" bind_type="tableName" input_field="Table_Name">表名</erp-search>
                    </div>
                    <div id="order_2" class="form-group">
                        <erp-select v_id="Field_Name" v-model="Field_Name" field_name="S_Code_Set.Field_Name" ismustselect="true" query_type="01" expression="02" v-bind:linkage="linkage" bind_type="FieldNameAll">字段名称</erp-select>
                    </div>
                    @*<div id="order_1" class="form-group show-order">
                            <erp-text field_name="S_Code_Set.Table_Name" query_type="01" expression="01" v-model="Table_Name" v_id="Table_Name">表名</erp-text>
                        </div>
                        <div id="order_2" class="form-group">
                            <erp-text field_name="S_Code_Set.Field_Name" query_type="01" expression="01" v-model="Field_Name" v_id="Field_Name">字段名称</erp-text>
                        </div>*@
                </form>
                <!-- SUBMIT BUTTON -->
                <div class="submitSearch">
                    <erp-button v-bind:btn_cb="Search" v_id="btnSearch" fa_icon="fa fa-search" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b">查询</erp-button>
                </div>
            </div>
        </div>
        <div class="col-md-12 col-sm-12 p-0">
            <erp-table showcheckbox="true" multiselect="true" v-bind:searchmodel="searchModel" v_id="CodeSetList" v-bind:tableurl="tableUrl" v-bind:columheaderseach="columHeaderSeach"></erp-table>
        </div>
    </div>
</div>
@Scripts.Render("~/Areas/SystemSet/Content/js/CodeSetList.js?v=" + ViewData["jsVersion"])
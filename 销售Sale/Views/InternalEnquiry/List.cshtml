@{
    Layout = "~/Views/Shared/_Index.cshtml";
}
<div id="apps" class="pt-5 pl-5 pr-5" v-cloak>
    <div class="row m-0">
        <div class="col-md-12 col-lg-12 col-sm-12 row-tit p-0 mb-5">
            <div id="T_Menu_Alias" class="col-md-2 col-sm-2 col-lg-2 p-tit">统购内部询价查询</div>
            <div class="col-md-10 col-sm-10 col-lg-10 p-btn pr-30">
                <div class="col-md-12 col-sm-12 col-lg-12 p-btn ">
                    <erp-button v-bind:btn_cb="New" v_id="btnNew" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b" fa_icon="fa fa-plus">新增</erp-button>
                    <erp-button v-bind:btn_cb="Update" v_id="btnUpdate" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b" fa_icon="fa fa-edit">编辑</erp-button>
                    <erp-button v-bind:btn_cb="Delete" v_id="btnDelete" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b" fa_icon="fa fa-trash">作废</erp-button>
                    <erp-button v-bind:btn_cb="Reply" v_id="btnReply" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b" fa_icon="fa fa-mail-reply">回复</erp-button>
                    <erp-button v-bind:btn_cb="CheckLook" v_id="btnCheckLook" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b" fa_icon="glyphicon glyphicon-eye-open">查看</erp-button>

                    <div class="btn-group" role="group">
                        <button type="button" class="btn-primary btn-rounded btn-sm btn-ef" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            其他
                            <span class="caret"></span>
                        </button>
                        <ul class="dropdown-menu spg-down">
                            <li><erp-button v-bind:btn_cb="Export" v_id="btnExport" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b" fa_icon="fa fa-mail-forward">导出</erp-button></li>
                             <li style="height:31px;"><erp-button v-bind:btn_cb="Set" v_id="btnSet" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b" fa_icon="fa fa-cog">设置</erp-button></li>

                        </ul>
                    </div>
                </div>
            </div>
            <div class="tit_down"><i class="fa fa-angle-double-down"></i></div>
        </div>
        <div class="col-md-12 col-sm-12 col-lg-12 pl-5 pr-0">
            <div class="tile_body searchHide clearfix">
                <form class="form-inline" id="searchForm">
                    <div id="order_1" class="form-group show-order">
                        <erp-searchtext class="search" field_name="C_Customer.Customer_Name" query_type="01" expression="01" v-model="Customer_Id" v_id="Customer_Id" control_type="customer" system_key="Sale">客户名称</erp-searchtext>
                   </div>
                    <div id="order_2" class="form-group show-order">
                        <erp-productrange field_name="M_Consult_Price_Bill_Detail.Product_Name" v-bind:productrange="productrange" v-model="Product_Name" query_type="01" expression="05" v_id="productrange">型号</erp-productrange>
                   </div>
                    <div id="order_3" class="form-group show-order">
                        <erp-searchtext class="search" field_name="M_Consult_Price_Bill_Detail.Brand_Name" query_type="01" expression="01" v_id="Brand_Name" v-model="Brand_Name" control_type="brand" system_key="Sale">品牌名称</erp-searchtext>
                   </div>
                    <div id="order_2" class="form-group show-order">
                        <erp-text field_name="M_Consult_Price_Bill_Detail.Package" query_type="01" expression="01" v-model="Package" v_id="Package">油脂包装</erp-text>

                    </div>
                    <div id="order_3" class="form-group show-order">
                        <erp-searchtext field_name="M_Consult_Price_Bill_Detail.Product_Classify_Name" query_type="01" expression="01" v-model="Product_Classify_Name" v_id="Product_Classify_Id" control_type="productBigClassify" system_key="Sale">品名</erp-searchtext>

                    </div>
                    <div id="order_4" class="form-group show-order">
                        <erp-multselect field_name="M_Consult_Price_Bill_Detail.Product_Area_Name" query_type="01" expression="04" v-model="Product_Area_Id" v_id="Product_Area_Id" v-bind:main_orderby="mainOrderBy" bind_type="CustomTable" table_name="B_Product_Area" system_key="Sale" v-bind:filed_seach="AreaFiledSearch" v-bind:filed_result="Arearesult" v-bind:input_field="AreaInputField" isselect="true">产地名称</erp-multselect>
                    </div>
                    <div id="order_4" class="form-group show-order">
                        <erp-searchtext class="search" field_name="B_Employee.Name" query_type="01" expression="01" v-model="Employee_Id" v_id="Employee_Id" control_type="employee" system_key="Sale">任务人</erp-searchtext>

                    </div>
                    <div id="order_5" class="form-group show-order">
                        <erp-text field_name="M_Consult_Price_Bill_Detail.Create_User" query_type="01" expression="01" v-model="Create_User" v_id="Create_User">创建用户</erp-text>

                    </div>
                    <div id="order_6" class="form-group show-order">
                        <erp-datedb field_name="M_Consult_Price_Bill_Detail.Create_Time" query_type="01" expression="02" v-model="Create_Time" v_id="Create_Time">创建日期</erp-datedb>
                    </div>
                    <div id="order_7" class="form-group show-order">
                        <erp-searchtext class="search" field_name="B_Department.Department_Name" query_type="01" expression="01" v-model="Department_Id" v_id="Department_Id" control_type="department" system_key="Sale">任务人部门</erp-searchtext>

                    </div>
                    <div id="order_8" class="form-group show-order">
                        <erp-multselect field_name="M_Consult_Price_Bill_Detail.Enquiry_Price_Status" query_type="01" expression="04" v_id="Enquiry_Price_Status" v-model="Enquiry_Price_Status" bind_type="StatusDictionary" system_key="SystemSet" dictionary_key="ENQUIRY_PRICE_STATUS">询价状态</erp-multselect>
                    </div>
                </form>
                <div class="submitSearch">
                    <span class="moreSearch">更多<i class="fa fa-angle-double-down"></i></span>
                    <erp-button v-bind:btn_cb="Search" v_id="btnSearch" fa_icon="fa fa-search" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b mb-10">查询</erp-button>
                    <erp-button v-bind:btn_cb="mClear" v_id="btnClear" fa_icon="fa fa-times" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b mb-10">清空</erp-button>
                </div>
            </div>
        </div>
        <div class="col-md-12 col-sm-12 col-lg-12 p-0">
            <erp-table pageing="true" istfootShow="true" v-bind:totalback="pagetotalback" v-bind:columrender="columrender" multiselect="false" showcheckbox="true" v-bind:searchmodel="searchModel" v_id="ConsultPriceBillList" v-bind:tableurl="tableUrl" v-bind:columheaderseach="columHeaderSeach"></erp-table>

        </div>
    </div>
</div>

@Scripts.Render("~/Areas/Sale/Content/js/InternalEnquiryList.js?v=" + ViewData["jsVersion"])


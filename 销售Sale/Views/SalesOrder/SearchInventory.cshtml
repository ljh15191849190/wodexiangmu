@{
    Layout = "~/Views/Shared/_Index.cshtml";
}

<div id="app" class="pt-5 pl-5 pr-5" v-cloak>
    <div class="row m-0">
        <div class="col-md-12 col-sm-12 col-lg-12 row-tit p-0 mb-5">
            <div id="T_Menu_Alias" class="col-md-2 col-sm-2 col-lg-2 p-tit">分配查询 </div>
            <div class="tit_down"><i class="fa fa-angle-double-down"></i></div>
        </div>
        <div class="col-md-12 col-sm-12 col-lg-12 pl-5 pr-5">
            <div class="tile_body searchHide clearfix">
                <form class="form-inline" id="searchForm">
                    <div id="order_7" class="form-group show-order">
                        <erp-productrange field_name="B_Product.Product_Name" v-bind:productrange="productrange" v-model="Product_Name" query_type="01" expression="05" v_id="productrange">型号</erp-productrange>
                    </div>
                    <div id="order_1" class="form-group show-order">
                        <erp-select v_id="Apply_Object_Code"field_name="B_Product.Apply_Object_Code"  query_type="01" expression="02" v-model="Apply_Object_Code"  dictionary_key="APPLY_OBJECT_CODE" bind_type="DataDictionary" v-bind:template_selection="ApplyObject">对象类型</erp-select>
                    </div>
                    <div id="order_2" class="form-group show-order Customer" style="display:none;">
                        <erp-searchtext class="search" field_name="B_Product.Object_Id" query_type="01" expression="01" v-model="Customer_Id" v_id="Customer_Id" control_type="customer" system_key="Sale">客户名称</erp-searchtext>
                    </div>
                    <div id="order_3" class="form-group show-order Applier" style="display:none;">
                        <erp-searchtext class="search" field_name="B_Product.Object_Id" query_type="01" expression="01" v-model="Employee_Id" v_id="Employee_Id" control_type="employee" system_key="Sale">任务人</erp-searchtext>
                   </div>
                    <div id="order_4" class="form-group show-order Department" style="display:none">
                        <erp-searchtext class="search" field_name="B_Product.Object_Id" v-model="Department_Id" query_type="01" expression="01" v_id="Department_Id" control_type="department" system_key="Sale">部门名称</erp-searchtext>
                    </div>
                    <div id="order_5" class="form-group show-order Area" style="display:none">
                        <erp-select ismustselect="false" field_name="B_Product.Object_Id" system_key="Sale" query_type="01" expression="02" v-model="Geography_Id" bind_type="CustomTable" table_name="B_Geography" v-bind:main_where="AreaWhere" v-bind:main_orderby="AreaOrder" v-bind:filed_result="AreaResult" v_id="Geography_Id">区域名称</erp-select>
                   </div>
                    <div id="order_3" class="form-group show-order">
                        <erp-searchtext class="search" field_name="B_Product.Brand_Name" query_type="01" expression="01" v_id="Brand_Name" v-model="Brand_Name" control_type="brand" system_key="Sale">品牌名称</erp-searchtext>

                    </div>
                    <div id="order_2" class="form-group show-order">
                        <erp-text field_name="B_Product.Package" query_type="01" expression="01" v-model="Package" v_id="Package">油脂包装</erp-text>

                    </div>
                    <div id="order_3" class="form-group show-order">
                        <erp-searchtext field_name="B_Product.Product_Classify_Name" query_type="01" expression="01" v-model="Product_Classify_Id" v_id="Product_Classify_Id" control_type="productBigClassify" system_key="Sale">品名</erp-searchtext>

                    </div>
                    <div id="order_4" class="form-group show-order">
                        <erp-multselect field_name="B_Product.Product_Area_Name" query_type="01" expression="04" v-model="Product_Area_Id" v_id="Product_Area_Id" v-bind:main_orderby="mainOrderBy" bind_type="CustomTable" table_name="B_Product_Area" system_key="Sale" v-bind:filed_seach="AreaFiledSearch" v-bind:filed_result="Arearesult" v-bind:input_field="AreaInputField" isselect="true">产地名称</erp-multselect>
                    </div>
                    <div id="order_6" class="form-group show-order">
                        <erp-select field_name="B_Product.Bill_Type" query_type="01" expression="02" v_id="Bill_Type" v-model="Bill_Type" bind_type="DataDictionary" system_key="Sale" dictionary_key="INVENTORY_DISTRIBUTE_BILL_TYPE">单据类型</erp-select>
                    </div>


                </form>
                <div class="submitSearch">
                    <span class="moreSearch">更多<i class="fa fa-angle-double-down"></i></span>
                    <erp-button v-bind:btn_cb="Search" v_id="btnSearch" fa_icon="fa fa-search" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b mb-10">查询</erp-button>
                    <erp-button v-bind:btn_cb="mClear" v_id="btnClear" fa_icon="fa fa-times" btn_type="btn-primary btn-rounded btn-ef btn-sm btn-ef-5 btn-ef-5b mb-10">清空</erp-button>
                </div>
            </div>
        </div>
        <div class="col-md-12 col-sm-12 col-lg-12 p-0">
            <erp-table v-bind:totalback="totalback" istfootshow="true" v-bind:columrender="columrender" pageing="true" multiselect="true" showcheckbox="true" v-bind:searchmodel="searchModel" v_id="DistributedList" v-bind:tableurl="tableUrl" v-bind:columheaderseach="columHeaderSeach"></erp-table>

        </div>
    </div>
</div>

@Scripts.Render("~/Areas/Sale/Content/js/SearchInventory.js?v=" + ViewData["jsVersion"])